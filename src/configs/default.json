{
  "branches": {
    "dev": {
      "protection": "full",
      "create": true
    },
    "stage": {
      "protection": "full",
      "create": true
    },
    "master": {
      "protection": "full",
      "create": true
    },
    "fix/*": {
      "protection": null,
      "create": false
    },
    "feat/*": {
      "protection": null,
      "create": false
    },
    "greenkeeper/*": {
      "protection": null,
      "create": false
    }
  },
  "status-checks": {
    "exclusions": []
  },
  "protection": {
    "full": {
      "required_status_checks": {
        "strict": true,
        "contexts": "$STATUS_CHECK_LIST"
      },
      "enforce_admins": true,
      "required_pull_request_reviews": {
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": false
      },
      "restrictions": {
        "users": [
          "???"
        ]
      }
    }
  }
}
